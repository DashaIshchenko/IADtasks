    "# Распакуем точки в 3 списка\n",
    "x, y, colors = zip(*points)\n",
    "# Определяем размеры фигуры\n",
    "fig, ax = plt.subplots() \n",
    "fig.set_size_inches(6.0, 6.0)\n",
    "# Рисуем результат\n",
