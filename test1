   "source": [
    "# Инициализация количества \"бросков\"\n",
    "num_points = 1000"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "36bf9ff0-d182-4fd6-92a6-1be89ff27a09",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Здесь мы \"бросаем дротики\" и подсчитываемколичество попаданий\n",
    "points = []\n",
    "hits = 0\n",
    "for _ in range(num_points):\n",
    "    x, y = random.random(), random.random()\n",
    "    if x*x + y*y < 1.0:\n",
    "        hits += 1\n",
    "        points.append((x, y, \"red\"))\n",
    "    else:\n",
    "        points.append((x, y, \"blue\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "268ef7c6-d5e3-495b-b5db-e72d0b433f51",
